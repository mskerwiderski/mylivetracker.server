    <sqlMap resource="ibatis-sql-maps/MessageVo.xml"/>
    <sqlMap resource="ibatis-sql-maps/SenderStateVo.xml"/>
    <sqlMap resource="ibatis-sql-maps/CardiacFunctionVo.xml"/>
    <sqlMap resource="ibatis-sql-maps/EmergencySignalVo.xml"/>      
    <sqlMap resource="ibatis-sql-maps/SenderVo.xml"/>   
    <sqlMap resource="ibatis-sql-maps/DemoTrackVo.xml"/>
